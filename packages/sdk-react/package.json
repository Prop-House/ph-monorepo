{
  "name": "@prophouse/sdk-react",
  "version": "1.0.22",
  "description": "Utilities that simplify interaction with the Prop House from React applications",
  "license": "GPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7.5.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean:build": "rimraf ./dist",
    "dev": "nodemon",
    "lint": "eslint src --ext .ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --check src/**/*.ts",
    "format:fix": "prettier --write src/**/*.ts",
    "typecheck": "tsc"
  },
  "peerDependencies": {
    "react": ">=16.13.1",
    "wagmi": ">=0.9.2"
  },
  "dependencies": {
    "@ethersproject/providers": "~5.7.0",
    "@prophouse/sdk": "1.0.27"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "react": "^17.0.2",
    "wagmi": "~1.2"
  }
}
